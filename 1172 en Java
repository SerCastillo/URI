
package Programas;

import java.util.Scanner;

public class URI1172 {

    public static void main(String[] args) {
        Scanner entrada = new Scanner(System.in);

        int i, n, X;
        for (i = 0; i <= 9; i++) {

            n=entrada.nextInt();
            if (n > 0) {
                System.out.println("X["+i+"] = "+n);
            } else {
                System.out.println("X[1]\n"+i);
            }
        }

    }

}
