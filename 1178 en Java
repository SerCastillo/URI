package Programas;

import java.util.Scanner;

public class URI1175 {

    public static void main(String[] args) {
        Scanner entrada = new Scanner(System.in);

        float n = 1;
        int i;
        n = entrada.nextInt();
        for (i = 0; i < 100; i++) {

            System.out.println("N[" + i + "] = " + n);
            n = n / 2;

        }

    }
}
